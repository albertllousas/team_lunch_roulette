{
  "name": "fun-with-vue",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Albert Ortiz <aortiz@thoughtworks.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "mapCoverage": true,
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bulma": "^0.6.1",
    "vee-validate": "^2.0.0-rc.26",
    "vue": "^2.4.4",
    "vue-blu": "^0.1.9",
    "vue-router": "^3.0.1",
    "vue2-autocomplete-js": "^0.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jest": "^21.2.1",
    "jest-mock-axios": "^1.0.21",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue-jest": "^1.3.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "vue-test-utils": "^1.0.0-beta.7",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
